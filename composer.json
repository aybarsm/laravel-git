{
    "name": "aybarsm/laravel-git",
    "description": "An intuitive service provider for Laravel and Laravel Zero, streamlining Git repository management both within and outside your application.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "aybarsm",
        "laravel",
        "git",
        "submodule",
        "mixins",
        "management"
    ],
    "homepage": "https://github.com/aybarsm/laravel-git",
    "authors": [
        {
            "name": "Murat Aybars",
            "email": "hello@aybarsm.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/process": "^10.0",
        "aybarsm/laravel-extended-support": "^1.0.7"
    },
    "require-dev": {
        "laravel/pint": "^1.12"
    },
    "autoload": {
        "psr-4": {
            "Aybarsm\\Laravel\\Git\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aybarsm\\Laravel\\Git\\GitServiceProvider"
            ],
            "aliases": {
                "Git": "Aybarsm\\Laravel\\Git\\Facades\\Git"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}